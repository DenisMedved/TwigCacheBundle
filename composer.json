{
    "name": "emanueleminotto/twig-cache-bundle",
    "description": "Symfony 2 Bundle for asm89/twig-cache-extension",
    "keywords": [
        "bundle",
        "cache",
        "symfony",
        "twig"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Emanuele Minotto",
            "email": "minottoemanuele@gmail.com"
        }
    ],
    "require": {
        "asm89/twig-cache-extension": "~1.0"
    },
    "require-dev": {
        "doctrine/cache": "~1.4",
        "phpunit/phpunit": "~4.4",
        "symfony/framework-bundle": "~2.6",
        "symfony/twig-bundle": "~2.6"
    },
    "autoload": {
        "psr-4": {
            "EmanueleMinotto\\TwigCacheBundle\\": "."
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "suggest": {
        "itinance/doctrine-cache-adapter-with-site-context": "a Cache Adapter used to respect the current site context in a white-label/multi-site environment"
    }
}
